{
  "version": 2,
  "alias": "nextjsorg.now.sh",
  "builds": [
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    {
      "src": "/_next/static/(chunks|runtime|commons)/.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/static/(favicon|images)/.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/_next/static/.*/pages/.*\\.js",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
